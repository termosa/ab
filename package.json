{
  "name": "ab",
  "version": "0.0.0",
  "description": "AngularJS boilerplate",
  "scripts": {
    "build": "npm run build:prod",
    "build:dev": "webpack --profile --config ./config/webpack-dev.config.js",
    "build:prod": "webpack --profile --config ./config/webpack-prod.config.js",
    "postinstall": "npm run build:prod",
    "start": "npm run start:dev",
    "start:dev": "WDS_DASHBOARD=1 webpack-dashboard -- webpack-dev-server --inline --hot --config ./config/webpack-dev.config.js",
    "start:prod": "WDS_DASHBOARD=1 webpack-dashboard -- webpack-dev-server --inline --config ./config/webpack-prod.config.js"
  },
  "author": "Stanislav Termosa <termosa.stanislav@gmail.com>",
  "license": "ISC",
  "dependencies": {
    "angular": "1.5",
    "angular-route": "1.5"
  },
  "devDependencies": {
    "bower": "1",
    "ng-annotate-loader": "0.1",
    "ng-cache-loader": "0.0",
    "webpack": "1.13",
    "webpack-dashboard": "0.1",
    "webpack-dev-server": "1.14",
    "webpack-manager": "2.1"
  }
}
